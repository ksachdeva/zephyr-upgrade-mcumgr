/ {
        aliases {
	        led0 = &myled0;
        };

        chosen {
                zephyr,uart-mcumgr = &uart0;
        };

        leds {
                compatible = "gpio-leds";

                myled0: led_0 {
                        gpios = <&gpio0 2 GPIO_ACTIVE_HIGH>;
                };
        };
};
